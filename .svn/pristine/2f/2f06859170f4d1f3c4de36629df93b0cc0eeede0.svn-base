using System;
using System.Collections.Generic;
//using System.Linq;
using System.Text;

namespace Jade.UI
{
	public class JOption
	{
		public JOption() { }
		public JOption(string t, object v)
		{
			this.Text = t;
			this.Value = v;
		}
		/// <summary>
		/// 文本
		/// </summary>
		public string Text { get; set; }

		/// <summary>
		/// 值
		/// </summary>
		public object Value { get; set; }

		public string GUID { get; set; }
	}
}
